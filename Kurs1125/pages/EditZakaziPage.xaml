<Page x:Class="Kurs1125.pages.EditZakaziPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Kurs1125.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EditZakaziPage">

    <Grid>
        <Button Height="20" Grid.RowSpan="2" VerticalAlignment="Top">Архив</Button>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="29*" />
            <RowDefinition Height="421*"/>
        </Grid.RowDefinitions>
        

        <Frame Content="{Binding CurrentPage}" Grid.Row="1" NavigationUIVisibility="Hidden" Foreground="#FFF5E6E6" BorderBrush="White" OpacityMask="#FFF9F9F9" Background="White" Grid.RowSpan="2"/>
    </Grid>
</Page>
